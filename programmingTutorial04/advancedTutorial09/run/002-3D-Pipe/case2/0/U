FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
		
    INLET
    {
        type 		timeVaryingParaboloidVelocity;
		Umax        2.0;
        Rm         	0.5;
        omega       5;
		n        (1 0 0);
		y        (0 1 0);
		z        (0 0 1);
		ctr      (0 0 0);
		value    (0 0 0); 
    }
    OUTLET
    {
        type            zeroGradient;
    }
    WALL
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

